import java.util.*;
import java.io.*;

public class CCC_J2010_J3 {
  
  
  public static void main(String args[]) throws IOException {
       Scanner scan = new Scanner(System.in); 
       while (true) {
         char a, b, c;
         
         String s = scan.next();
         a = s.charAt[0];
         if (a== '7') 
           break;
           
         b = s.charAt[1];
         if (a != '1' && a != '2') 
           c = s.charAt[2];
         
         int av = Integer.parse(a);
         inv bv = Integer.parse(b);
         
         switch (a) {
           case '1':
             break;
           case '2':
             System.out.println(""+ a);
             break;
           case '3':
             a = "" + av + bv;
             break;
           case '4':
             a = "" + av * bv;
             break;
           case '5':
             a = "" + av - bv;
             break;
           case '6':
             a = (int)av/bv;
             break;
         }
           
       }
         
  }
  
}
